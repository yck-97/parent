<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yck.user.mapper.UserMapper">
    <select id="queryUserList" resultType="com.yck.user.dto.UserDTO" parameterType="java.lang.String">
        SELECT
            id,
            user_name "userName",
            user_age "userAge",
            password,
            realname
        from user_info_t
        where
            user_name = #{userName}
    </select>
</mapper>
